<sdf version='1.5'>
  <model name='1p_2link'>
    <self_collide>true</self_collide>

    <link name='base_link'>
      <gravity>0</gravity>
      <pose frame=''>0 0 0.55 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.0076</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0004</iyy>
          <iyz>0</iyz>
          <izz>0.0076</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <geometry>
          <box>
            <size>0.0499 0.3 0.05</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x03</collide_bitmask>
          </contact>
        </surface>
      </collision>
      <visual name='base_link_visual'>
        <geometry>
          <box>
            <size>0.05 0.3 0.05</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/FlatBlack</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
    </link>

    <joint name='anchor' type='fixed'>
      <child>base_link</child>
      <parent>world</parent>
      <axis>
        <xyz>0 0 0</xyz>
      </axis>
    </joint>

    <link name='base_ft_link'>
      <gravity>0</gravity>
      <pose frame=''>0.025 0 0.55 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00001</iyy>
          <iyz>0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
      <collision name='base_ft_collision'>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x00</collide_bitmask>
          </contact>
        </surface>
      </collision>
      <visual name='base_ft_visual'>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
    </link>

    <joint name='base_ft_joint' type='fixed'>
      <child>base_ft_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 0</xyz>
      </axis>
    </joint>

    <link name='first_link'>
      <gravity>0</gravity>
      <pose>0.025 0 0.55 0 0 0</pose>
      <inertial>
        <pose>0.1 0 0.0125 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.00025</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00324</iyy>
          <iyz>0</iyz>
          <izz>0.0034</izz>
        </inertia>
      </inertial>
      <collision name='first_link_collision'>
        <pose frame=''>0.1 0 0.0125 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.05 0.025</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x01</collide_bitmask>
          </contact>
        </surface>
      </collision>
      <visual name='first_link_visual'>
        <pose frame=''>0.1 0 0.0125 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.05 0.025</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Yellow</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
    </link>

    <joint name='first_link_ft_joint' type='fixed'>
      <child>first_link</child>
      <parent>base_ft_link</parent>
      <axis>
        <xyz>0 0 0</xyz>
      </axis>
      <sensor name='first_link_ft' type='force_torque'>
        <always_on>1</always_on>
        <update_rate>100</update_rate>
        <pose>0 0 0 0 0 0</pose>
        <plugin name='first_link_ft_plugin' filename='libgazebo_yarp_forcetorque.so'>
          <yarpConfigurationFile>model://1p_2link/conf/first_link_ft.ini</yarpConfigurationFile>
          <frame>child</frame>
          <measured_direction>child_to_parent</measured_direction>
        </plugin>
      </sensor>
    </joint>

    <link name='second_link'>
      <gravity>0</gravity>
      <pose>0.025 0 0.55 0 0 0</pose>
      <inertial>
        <pose>0.1 0 -0.0125 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.00025</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00324</iyy>
          <iyz>0</iyz>
          <izz>0.0034</izz>
        </inertia>
      </inertial>
      <collision name='second_link_collision'>
        <pose frame=''>0.1 0 -0.0125 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.05 0.025</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x01</collide_bitmask>
          </contact>
        </surface>
      </collision>
      <collision name='second_link_collision_1'>
        <pose frame=''>0.225 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.0002</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <collide_bitmask>0x02</collide_bitmask>
          </contact>
        </surface>
      </collision>
      <visual name='second_link_visual'>
        <pose frame=''>0.1 0 -0.0125 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.05 0.025</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Blue</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
        <visual name='second_link_visual_1'>
          <pose frame=''>0.225 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 0.05 0.0002</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/RedTransparentOverlay</name>
              <uri>file://media/materials/scripts/gazebo.material</uri>
            </script>
          </material>
        </visual>
    </link>

      <joint name='first_joint' type='prismatic'>
        <child>second_link</child>
        <parent>first_link</parent>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>0</lower>
            <upper>0.15</upper>
            <effort>30</effort>
            <velocity>10</velocity>
          </limit>
          <!--dynamics>
            <spring_reference>0</spring_reference>
            <spring_stiffness>0</spring_stiffness>
          </dynamics>
          <use_parent_model_frame>1</use_parent_model_frame-->
        </axis>
      </joint>

      <link name='second_link_handle'>
        <gravity>0</gravity>
        <pose frame=''>0.25 0 0.55 0 0 0</pose>
        <inertial>
          <mass>1</mass>
          <inertia>
            <ixx>0.00001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.00001</iyy>
            <iyz>0</iyz>
            <izz>0.00001</izz>
          </inertia>
        </inertial>
        <collision name='second_link_handle_collision'>
          <pose frame=''>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <length>0.1</length>
              <radius>0.01</radius>
            </cylinder>
          </geometry>
          <surface>
            <contact>
              <collide_bitmask>0x03</collide_bitmask>
            </contact>
          </surface>
        </collision>
        <visual name='second_link_handle_visual'>
          <geometry>
            <cylinder>
              <length>0.1</length>
              <radius>0.01</radius>
            </cylinder>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Black</name>
              <uri>file://media/materials/scripts/gazebo.material</uri>
            </script>
          </material>
        </visual>
      </link>

      <joint name='handle_link_ft_joint' type='fixed'>
        <child>second_link_handle</child>
        <parent>second_link</parent>
        <axis>
          <xyz>0 0 0</xyz>
        </axis>
        <sensor name='second_link_ft' type='force_torque'>
          <always_on>1</always_on>
          <update_rate>100</update_rate>
          <pose>0 0 0 0 0 0</pose>
          <plugin name='second_link_ft_plugin' filename='libgazebo_yarp_forcetorque.so'>
            <yarpConfigurationFile>model://1p_2link/conf/second_link_ft.ini</yarpConfigurationFile>
            <frame>child</frame>
            <measured_direction>child_to_parent</measured_direction>
          </plugin>
        </sensor>
      </joint>

      <plugin name="controlboard" filename="libgazebo_yarp_controlboard.so">
        <yarpConfigurationFile>model://1p_2link/conf/gazebo_controlboard.ini</yarpConfigurationFile>
        <initialConfiguration>0.0</initialConfiguration>
      </plugin>

      <plugin name='second_link_handle_multi_ext_wrench' filename='libgazebo_yarp_multiexternalwrench.so'>
        <robotNamefromConfigFile>model://1p_2link/conf/gazebo_1p_2link_robotname.ini</robotNamefromConfigFile>
      </plugin>

      <plugin name='access_link_pose' filename='libgazebo_yarp_accesslinkpose.so'>
        <yarpConfigurationFile>model://1p_2link/conf/gazebo_accesslinkpose.ini</yarpConfigurationFile>
      </plugin>

  </model>
</sdf>
